{
  "name": "vue-components-init",
  "description": "vue components project",
  "version": "1.0.0",
  "author": "DreamyTZK",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack server --open --hot",
    "build": "npm run fix && cross-env NODE_ENV=production webpack --progress",
    "test": "cross-env NODE_ENV=development webpack --progress",
    "fix": "npm run lint-fix && lint:prettier && lint:stylelint",
    "format": "prettier --write ./src",
    "lint": "eslint ./src --ext .vue,.js,.ts,.tsx",
    "lint-fix": "eslint --fix ./src --ext .vue,.js,.ts,.tsx",
    "lint:eslint": "eslint \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --fix \"src/**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/"
  },
  "dependencies": {
    "@better-scroll/core": "^2.4.2",
    "@better-scroll/observe-dom": "^2.4.2",
    "@better-scroll/observe-image": "^2.4.2",
    "@better-scroll/pull-down": "^2.4.2",
    "@better-scroll/pull-up": "^2.4.2",
    "axios": "^0.24.0",
    "qs": "^6.10.1",
    "vant": "3",
    "vue": "^3.2.22"
  },
  "browserslist": {
    "development": [
      "> 1%",
      "last 2 versions",
      "Android >= 4.0",
      "iOS >= 8"
    ],
    "production": [
      "> 1%",
      "last 2 versions",
      "Android >= 4.0",
      "iOS >= 8"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@stylelint/postcss-css-in-js": "^0.37.2",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "@vue/compiler-sfc": "^3.2.22",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-standard": "^6.1.0",
    "babel-loader": "8.2.3",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-import": "^1.13.3",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-vue": "^8.2.0",
    "eslint-webpack-plugin": "^3.1.1",
    "file-loader": "^6.2.0",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "mini-css-extract-plugin": "^2.4.5",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "postcss-html": "^1.3.0",
    "postcss-less": "^5.0.0",
    "postcss-loader": "^6.2.0",
    "postcss-preset-env": "^7.0.1",
    "postcss-px-to-viewport": "^1.1.1",
    "postcss-scss": "^4.0.2",
    "prettier": "^2.5.1",
    "sass": "^1.43.4",
    "sass-loader": "^12.3.0",
    "standard-version": "^9.3.2",
    "stylelint": "^14.1.0",
